<%@ Register TagPrefix="dx" Namespace="DevExpress.Data" Assembly="DevExpress.Data.v9.3, Version=9.3.2.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" %>
<%@ Register TagPrefix="dxrp" Namespace="DevExpress.Web.ASPxRoundPanel" Assembly="DevExpress.Web.v9.3, Version=9.3.2.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" %>
<%@ Register Assembly="DevExpress.Web.v9.3, Version=9.3.2.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" Namespace="DevExpress.Web.ASPxPanel" TagPrefix="dx" %>

<dxrp:ASPxRoundPanel BackColor="#FFFFFF" runat="server" CssPostfix="Aqua" CssFilePath="{0}/styles.css" 
    SpriteCssFilePath="{0}/sprite.css">
    <Border BorderWidth="1px" BorderStyle="Solid" BorderColor="#AECAF0" />
    <ContentPaddings Padding="14px" />
    <HeaderStyle BackColor="#E0EDFF">
        <BorderBottom BorderStyle="Solid" BorderWidth="1px" BorderColor="#AECAF0" />
    </HeaderStyle>
    <%-- Header parts --%>
    <HeaderContent>
        <BackgroundImage Repeat="RepeatX" VerticalPosition="Top" ImageUrl="Web/rpHeaderBackground.gif" />
    </HeaderContent>
    <HeaderLeftEdge>
        <BackgroundImage Repeat="RepeatX" VerticalPosition="Top" ImageUrl="Web/rpHeaderBackground.gif" />
    </HeaderLeftEdge>
    <HeaderRightEdge>
        <BackgroundImage Repeat="RepeatX" ImageUrl="Web/rpHeaderBackground.gif" VerticalPosition="Top" />
    </HeaderRightEdge>
    <%-- Middle parts --%>
    <TopEdge>
        <BackgroundImage Repeat="RepeatX" VerticalPosition="Top" ImageUrl="Web/rpTopEdge.gif" />
    </TopEdge>
    <NoHeaderTopEdge BackColor="#FFFFFF">
        <BackgroundImage Repeat="RepeatX" VerticalPosition="Top" ImageUrl="Web/rpNoHeaderTopEdge.gif" />
    </NoHeaderTopEdge>
    <BottomEdge>
        <BackgroundImage Repeat="RepeatX" VerticalPosition="Bottom" ImageUrl="Web/rpBottomEdge.gif" />
    </BottomEdge>
    
    <LeftEdge>
        <BackgroundImage Repeat="RepeatY" VerticalPosition="Top" ImageUrl="Web/rpLeftEdge.gif" />
    </LeftEdge>
    <RightEdge>
        <BackgroundImage Repeat="RepeatY" VerticalPosition="Top" ImageUrl="Web/rpRightEdge.gif" />
    </RightEdge>
</dxrp:ASPxRoundPanel>
