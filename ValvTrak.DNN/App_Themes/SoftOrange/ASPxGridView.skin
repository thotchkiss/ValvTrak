<%@ Register TagPrefix="dx" Namespace="DevExpress.Data" Assembly="DevExpress.Data.v10.2" %>
<%@ Register TagPrefix="dxwgv" Namespace="DevExpress.Web.ASPxGridView" Assembly="DevExpress.Web.ASPxGridView.v10.2" %>

<dxwgv:ASPxGridView runat="server" CssPostfix="SoftOrange" CssFilePath="{0}/styles.css" >
    <SettingsPager CurrentPageNumberFormat="{0}">
    </SettingsPager>
    <SettingsLoadingPanel Text=""></SettingsLoadingPanel>

    <Images SpriteCssFilePath="{0}/sprite.css">
        <LoadingPanel Url="GridView/Loading.gif"></LoadingPanel>
        <LoadingPanelOnStatusBar Url="GridView/gvLoadingOnStatusBar.gif"></LoadingPanelOnStatusBar>
    </Images>
    <ImagesFilterControl>
        <LoadingPanel Url="Editors/Loading.gif"></LoadingPanel>
    </ImagesFilterControl>

    <Styles CssPostfix="SoftOrange" CssFilePath="{0}/styles.css" GroupButtonWidth="28">
        <LoadingPanel ImageSpacing="10px"></LoadingPanel>
        <Header SortingImageSpacing="5px" ImageSpacing="5px"></Header>
    </Styles>
    <StylesEditors>
        <ProgressBar Height="29px">
        </ProgressBar>
        <CalendarHeader Spacing="1px">
        </CalendarHeader>
    </StylesEditors>    
    <Paddings Padding="1px" />
</dxwgv:ASPxGridView>

