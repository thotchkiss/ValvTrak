  <DataSets>
    <DataSet Name="ValvTrak">
      <Fields>
        <Field Name="Client">
          <DataField>Client</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Location">
          <DataField>Location</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Latitude">
          <DataField>Latitude</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Longitude">
          <DataField>Longitude</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ValvTrak</DataSourceName>
        <CommandText>select clients.name as [Client],
           clientlocations.clientlocationid as [ID],
           clientlocations.name as [Location],
           clientlocations.latitude as [Latitude],
           clientlocations.longitude as [Longitude]
from clients
inner join clientlocations on clients.clientid = clientlocations.clientid
where clients.active = 1 and clientlocations.active = 1
and (clients.clientid = @ClientId or @ClientId is null)
and (clientlocations.clientlocationid = @ClientLocationId or @ClientLocationId is null)
order by clients.name, clients.parentid, clientlocations.name</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ClientId">
            <Value>=Parameters!ClientId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ClientLocationId">
            <Value>=Parameters!ClientLocationId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Clients">
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Value">
          <DataField>Value</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ValvTrak</DataSourceName>
        <CommandText>select '&lt;All&gt;' as [Name], null as [Value]
union
select name, clientid from clients where active = 1
order by [Name]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ClientLocations">
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Value">
          <DataField>Value</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ValvTrak</DataSourceName>
        <CommandText>select '&lt;All&gt;' as [Name], null as [Value]
union
select name, clientlocationid from clientlocations where active = 1
and (clientid = @ClientId or @ClientId is null)
order by [Name]</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ClientId">
            <Value>=Parameters!ClientId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
